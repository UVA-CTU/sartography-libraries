{
  "name": "sartography-libraries",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:coverage": "ng test --codeCoverage=true --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "patch-version": "cd projects/sartography-workflow-lib && npm version patch && cd ../..",
    "republish": "ng build sartography-workflow-lib && cd dist/sartography-workflow-lib && npm publish && cd ../..",
    "bump": "npm run lint && npm run patch-version && npm run republish",
    "ci": "npm run lint && npm run test:coverage && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.7",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "^9.1.7",
    "@angular/compiler": "^9.1.7",
    "@angular/core": "^9.1.7",
    "@angular/flex-layout": "^9.0.0-beta.30",
    "@angular/forms": "^9.1.7",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "^9.1.7",
    "@angular/platform-browser-dynamic": "^9.1.7",
    "@angular/router": "^9.1.7",
    "@ngx-formly/core": "^5.6.1",
    "@ngx-formly/material": "^5.6.1",
    "ngx-device-detector": "^1.4.1",
    "ngx-file-drop": "^8.0.8",
    "ngx-markdown": "^9.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.13.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.7",
    "@angular-devkit/build-ng-packagr": "^0.900.7",
    "@angular/cli": "^9.1.6",
    "@angular/compiler-cli": "^9.1.7",
    "@angular/language-service": "^9.1.7",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.39",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.1.3",
    "protractor": "^5.4.3",
    "ts-node": "~8.6.2",
    "tslint": "~6.0.0",
    "typescript": "~3.7.5"
